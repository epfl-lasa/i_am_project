<launch>
  <include
      file = "$(find iiwa_toolkit)/launch/passive_track_gazebo_dual.launch">
      <arg name="robot_name_1" value="iiwa1"/>
      <arg name="robot_name_2" value="iiwa2"/>
      <arg name="iiwa_model_1" value="14"/>
      <arg name="iiwa_model_2" value="14"/>
      <arg name="position_1" value="-x -0.55 -y -0.65"/>
      <arg name="position_2" value="-x 0.55 -y 0.65 -Y 3.1415926536"/>
  </include>

  <rosparam file="$(find i_am_project)/config/1v1_params.yaml" />

  <node name="initialization" pkg="i_am_project" type="AH_init" output="screen"/>
  <node name="master" pkg="i_am_project" type="AH_master"   output="screen"/>
  <node name="estimator" pkg="i_am_predict" type="estimate_sim"  output="screen"/>

</launch>

