<robot name="big_box">
  <link name="big_box">
    
    <gazebo reference="world">
      <static>true</static>
    </gazebo>
    <gazebo reference = "world">
      <turnGravityOff>false</turnGravityOff>
    </gazebo>
    <inertial>
      <origin xyz="0.0 0 0" /> 
      <mass value="15" />
      <inertia  ixx="15.0" ixy="0.0"  ixz="0.0"  iyy="15.0"  iyz="0.0"  izz="15.0" />
    </inertial>
    <visual>
      <origin xyz="0.0 0 0"/>
      <geometry>
        <box size="0.8 1.6 0.1" />
      </geometry>
    </visual>
    <surface>
        <friction>
          <ode>
            <mu>0.001</mu>
            <mu2>0.001</mu2>
          </ode>
        </friction>
    </surface>
    <collision>
      <origin xyz="0.0 0 0"/>
      <geometry>
        <box size="0.8 1.6 0.1" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="big_box">
    <material>
      <ambient>0.2 0.3 0.4 1</ambient>
      <diffuse>0.5 0.4 0.5 1</diffuse>
      <specular>0 0 0 0</specular>
      <emissive>0 0 0 1</emissive>
    </material>
  </gazebo>
</robot>