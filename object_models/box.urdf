<robot name="simple_box">
  <link name="my_box">
    
    <!-- <gazebo reference="world">
      <static>false</static>
    </gazebo> -->
    <gazebo reference = "world">
      <turnGravityOff>true</turnGravityOff>
    </gazebo>
    <inertial>
      <origin xyz="0.0 0 0" /> 
      <mass value="0.05" />
      <inertia  ixx="0.00033" ixy="0.0"  ixz="0.0"  iyy="0.00033"  iyz="0.0"  izz="0.00033" />
    </inertial>
    <visual>
      <origin xyz="0.0 0 0"/>
      <geometry>
        <box size="0.23 0.26 0.4" />
      </geometry>
    </visual>
    <surface>
        <friction>
          <ode>
            <mu>0.001</mu>
            <mu2>0.001</mu2>
          </ode>
        </friction>
    </surface>
    <collision>
      <origin xyz="0.0 0 0"/>
      <geometry>
        <box size="0.23 0.26 0.4" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
    <material>
      <ambient>0.1 0.1 0.1 1</ambient>
      <diffuse>0.1 0.1 0.2 1</diffuse>
      <specular>0 0 0 0</specular>
      <emissive>0 0 0 1</emissive>
    </material>
  </gazebo>
</robot>